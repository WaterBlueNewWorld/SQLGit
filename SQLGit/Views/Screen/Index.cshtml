                <button class="btn btn-default" id="btnCrearCamposNuevos">crear campos nuevos</button>
            </div>
        </div>
    
        <div class="row">
            <button class="btn btn-default" id="btnConsultar">Consultar</button>
            <button class="btn btn-default" id="btnRelaciones">Relaciones</button>
            <button class="btn btn-default" id="btnFaltantes">Faltantes</button>
            <button class="btn btn-default" id="btnDistintos">Distintos</button>
            <button class="btn btn-default" id="btnSobrantes">Sobrantes</button>
            <button class="btn btn-default" id="btnSalir">Salir</button>
        
        
        </div>
    
    </div>

    </div>
