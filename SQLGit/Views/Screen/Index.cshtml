@using DevExpress.Web.Mvc.UI

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
   <div class="card" style=" width:100%; padding:0;">
           <div class="col-md-12" style="padding:0;" align="center">
               <h3 style="border-top:3px solid #009688; border-bottom:3px solid #009688; margin:0;">Pizaña and Aviña Assemble</h3>
           </div>
       </div> 
    <div class="card">
        <div class="row">
            <button id="btnCambiardb" class="btn btn-default">Seleccionar db</button>
        </div>
    </div>
    
    <div class="row">
        <div class="col">
            @Html.Action("TablaDistinta")
        </div> 
        <div class="col">
            @Html.Action("EsquemaOrigen")
        </div>
        
    </div>
    <div class="row">
